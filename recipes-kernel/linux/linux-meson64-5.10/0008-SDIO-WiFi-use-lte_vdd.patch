From fe7a67f0e1e15cd7d7a0911aff35d87f40b2d07c Mon Sep 17 00:00:00 2001
From: changzhijie <elecboy@126.com>
Date: Sat, 18 Sep 2021 15:22:36 +0800
Subject: [PATCH 8/8] SDIO WiFi use lte_vdd

---
 .../dts/amlogic/meson-sm1-sports-cle-v4.dts     | 17 +++++++++++++++++
 1 file changed, 17 insertions(+)

diff --git a/arch/arm64/boot/dts/amlogic/meson-sm1-sports-cle-v4.dts b/arch/arm64/boot/dts/amlogic/meson-sm1-sports-cle-v4.dts
index d2022e760939..40c93e09697c 100644
--- a/arch/arm64/boot/dts/amlogic/meson-sm1-sports-cle-v4.dts
+++ b/arch/arm64/boot/dts/amlogic/meson-sm1-sports-cle-v4.dts
@@ -45,6 +45,19 @@ vddcpu: regulator-vddcpu {
 		regulator-always-on;
 	};
 
+	lte_vdd: regulator-tflash_vdd {
+                compatible = "regulator-fixed";
+
+                regulator-name = "LTE_VDD";
+                regulator-min-microvolt = <3300000>;
+                regulator-max-microvolt = <3300000>;
+
+                gpio = <&gpio GPIOA_11 GPIO_ACTIVE_HIGH>;
+                enable-active-high;
+                vin-supply = <&dc_in>;
+                regulator-always-on;
+        };
+
 	nrf_pwr: regulator-nrf_pwr {
 		compatible = "regulator-fixed";
 		regulator-name = "NRF_PWR";
@@ -220,3 +233,7 @@ &gpio_ao {
 		/* GPIOE */
 		"", "", "";
 };
+
+&sd_emmc_a {
+       vmmc-supply = <&lte_vdd>;
+};
-- 
